        android:layout_height="7dp" />


    <ImageView
        android:id="@+id/iv_search"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:src="@drawable/alarm_main"/>

    <View
        android:layout_width="match_parent"
        android:layout_height="7dp" />


